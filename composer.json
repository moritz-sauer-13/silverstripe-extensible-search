{
	"name": "moritz-sauer-13/silverstripe-extensible-search",
	"description": "This module allows user customisation and developer extension of a search page instance, including analytics and suggestions.",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"search",
		"full-text"
	],
	"homepage": "https://github.com/nglasl/silverstripe-extensible-search",
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Moritz Sauer",
		"email": "ms@tietge.com"
	}],
	"require": {
		"silverstripe/cms": "^5",
		"silverstripe/framework": "^5"
	},
	"autoload": {
		"psr-4": {
			"nglasl\\extensible\\": [
				"src/controllers/",
				"src/extensions/",
				"src/objects/",
				"src/pages/",
				"src/services/",
				"src/tasks/"
			],
			"nglasl\\extensible\\tests\\": "tests/"
		}
	},
	"extra": {
		"expose": [
			"client"
		]
	}
}
